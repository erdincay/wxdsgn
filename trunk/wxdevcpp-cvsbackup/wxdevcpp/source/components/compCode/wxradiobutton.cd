L4=~~~~~var
L5=~~~~~strStyle,parentName,strAlignment:String;
L6=begin
L7=~~~~~Result:='';
L8=~~~~if (self.Parent is TForm) or (self.Parent is TWxSizerPanel) then
L9=~~~~~~~parentName:='this'
L10=~~~~else
L11=~~~~~~~parentName:=self.Parent.name;
L12=
L13=~~~~strStyle:=GetRadioButtonSpecificStyle(self.Wx_GeneralStyle,Wx_RadioButtonStyle);
L14=
L15=~~~~Result:=Format('%s =  new %s(%s, %s ,_("%s") ,wxPoint(%d,%d),wxSize(%d,%d) %s);',[self.Name,self.wx_Class,parentName,GetWxIDString(Wx_IDName,self.Wx_IDValue),self.Caption,self.Left,self.Top,self.width,self.Height,strStyle] );
L16=
L17=~~~~if trim(self.Wx_ToolTip) <> '' then
L18=~~~~~~~~Result:=Result + #13+Format('%s->SetToolTip(wxT("%s"));',[self.Name,self.Wx_ToolTip]);
L19=
L20=~~~~if self.Wx_Hidden then
L21=~~~~~~~~Result:=Result + #13+Format('%s->Show(false);',[self.Name]);
L22=
L23=~~~~if not Wx_Enabled then
L24=~~~~~~~~Result:=Result + #13+Format('%s->Enable(false);',[self.Name]);
L25=
L26=~~~~if trim(self.Wx_HelpText) <> '' then
L27=~~~~~~~~Result:=Result +#13+Format('%s->SetHelpText(_("%s"));',[self.Name,self.Wx_HelpText]);
L28=
L29=~~~~if self.Checked then
L30=~~~~~~~~Result:=Result +#13+Format('%s->SetValue(true);',[self.Name]);
L31=~~~~if(self.Parent is TWxSizerPanel) then
L32=~~~~begin
L33=~~~~~~~~strAlignment:=SizerAlignmentToStr(Wx_HorizontalAlignment) + ' | '+ SizerAlignmentToStr(Wx_VerticalAlignment) +' | wxALL';
L34=~~~~~~~~if wx_ControlOrientation = wxControlVertical then
L35=~~~~~~~~~~~~strAlignment:=SizerAlignmentToStr(Wx_HorizontalAlignment)+ ' | wxALL';
L36=
L37=~~~~~~~~~if wx_ControlOrientation = wxControlHorizontal then
L38=~~~~~~~~~~~~~strAlignment:=SizerAlignmentToStr(Wx_VerticalAlignment)+ ' | wxALL';
L39=
L40=
L41=~~~~~~~~~Result:=Result +#13+Format('%s->Add(%s,%d,%s,%d);',[self.Parent.Name,self.Name,self.Wx_StrechFactor,strAlignment,self.Wx_Border]);
L42=~~~~end;
L43=
L44=end;
